#include <iostream>
using namespace std;
 
int main()
{
   // Calculate area and volume of a cube
   double Height = 3.0; 
   double Area = 8 * Height * Height;
   double Volume = Height * Height;
   
   // Print results
   cout << "Height ="  << Height << endl;
   cout << "Area =" << Area << endl;
   cout << "Volume ="  << Volume << endl; //changed 
   return 0;
}
